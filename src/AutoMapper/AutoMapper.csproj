<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Simple AutoMapper</Title>
    <Description>Simple classes AutoMapper which does not require any configuration to use</Description>
    <PackageProjectUrl>https://github.com/Suzyklementy/simple-auto-mapper</PackageProjectUrl>
    <PackageId>SimpleAutoMapperSuzyklementy</PackageId>
  </PropertyGroup>

</Project>
